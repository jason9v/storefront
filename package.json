{
  "name": "store-hub",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "export": "next build",
    "build:gh-pages": "cross-env GITHUB_PAGES=true next build",
    "deploy": "npm run build:gh-pages && gh-pages -d out",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:next": "next lint",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\""
  },
  "dependencies": {
    "@designbycode/tailwindcss-text-shadow": "^2.2.1",
    "@eslint/eslintrc": "^3.3.1",
    "@reduxjs/toolkit": "^2.10.1",
    "@tanstack/react-query": "^5.90.7",
    "@types/js-cookie": "^3.0.6",
    "axios": "^1.13.2",
    "date-fns": "^4.1.0",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^4.0.0",
    "next": "16.0.1",
    "next-intl": "^4.5.0",
    "next-themes": "^0.4.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
